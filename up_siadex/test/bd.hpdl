(define (domain bloques_1-domain)
 (:requirements
   :strips
   :typing
   :fluents
   :htn-expansion
 )
 (:types
    bloque superficie - object
 )
 (:constants
   mesa - superficie
 )
 (:predicates
  (manovacia)
  (libre ?x - bloque)
  (cogido ?x - bloque)
  (sobremesa ?x - bloque)
  (sobre ?x - bloque ?y - bloque)
  (distinto ?x - object ?y - object)
 )
 (:functions (igual ?x - object ?y - object)
 )
 (:task sobre
  :parameters (?x - object ?y - object )
  (:method sobre_poner_encima
   :precondition ()
   :tasks (
    (limpiar ?x - object )
    (limpiar ?y - object )
    (colocar ?x - object ?y - object )
   )
  )
 )
 (:task limpiar
  :parameters (?x - object )
  (:method limpiar_limpiar_mesa
   :precondition (and
    (< 0 (igual ?x mesa))
   )
   :tasks (
   )
  )
  (:method limpiar_limpiar_ocupado
   :precondition (and
    (sobre ?y ?x)
   )
   :tasks (
    (limpiar ?y - object )
    (colocar ?y - object mesa )
   )
  )
  (:method limpiar_limpiar_libre
   :precondition (and
    (libre ?x)
   )
   :tasks (
   )
  )
 )
 (:task colocar
  :parameters (?x - object ?y - object )
  (:method colocar_colocar
   :precondition ()
   :tasks (
    (primero_coge ?x - bloque )
    (despues_deja ?x - bloque ?y - object )
   )
  )
 )
 (:task primero_coge
  :parameters (?x - bloque )
  (:method primero_coge_cogelo_de_la_mesa
   :precondition (and
    (sobremesa ?x)
   )
   :tasks (
    (coger ?x - bloque )
   )
  )
  (:method primero_coge_cogelo_de_la_pila
   :precondition (and
    (sobre ?x ?y)
   )
   :tasks (
    (desapilar ?x - bloque ?y - bloque )
   )
  )
 )
 (:task despues_deja
  :parameters (?x - bloque ?y - object )
  (:method despues_deja_dejalo_en_la_mesa
   :precondition (and
    (< 0 (igual ?y mesa))
   )
   :tasks (
    (dejar ?x - bloque )
   )
  )
  (:method despues_deja_dejalo_en_la_pila
   :precondition (and
    (<= (igual ?y mesa) 0)
   )
   :tasks (
    (apilar ?x - bloque ?y - bloque )
   )
  )
 )
 (:action coger
  :parameters (?x - bloque )
  :precondition (and
   (and (sobremesa ?x) (libre ?x) (manovacia))
  )
  :effect (and
   (not (sobremesa ?x))(not (libre ?x))(not (manovacia))(cogido ?x)
  )
 )
 (:action dejar
  :parameters (?x - bloque )
  :precondition (and
   (cogido ?x)
  )
  :effect (and
   (sobremesa ?x)(libre ?x)(manovacia)(not (cogido ?x))
  )
 )
 (:action apilar
  :parameters (?x - bloque ?y - bloque )
  :precondition (and
   (and (cogido ?x) (libre ?y))
  )
  :effect (and
   (not (cogido ?x))(not (libre ?y))(libre ?x)(sobre ?x ?y)(manovacia)
  )
 )
 (:action desapilar
  :parameters (?x - bloque ?y - bloque )
  :precondition (and
   (and (manovacia) (libre ?x) (sobre ?x ?y))
  )
  :effect (and
   (cogido ?x)(libre ?y)(not (libre ?x))(not (sobre ?x ?y))(not (manovacia))
  )
 )
)
